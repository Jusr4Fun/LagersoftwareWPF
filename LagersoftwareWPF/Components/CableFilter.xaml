<UserControl x:Class="LagersoftwareWPF.Components.CableFilter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LagersoftwareWPF.Components"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ComboBox
            Grid.Row="0"
            Grid.Column="0"
            Height="20"
            Width="125"
            />
        <ComboBox
            Grid.Row="1"
            Grid.Column="0"
            Height="20"
            Width="125"
            />
        <ComboBox
            Grid.Row="0"
            Grid.Column="1"
            Height="20"
            Width="125"
            />
        <ComboBox
            Grid.Row="1"
            Grid.Column="1"
            Height="20"
            Width="125"
            />
    </Grid>
</UserControl>
